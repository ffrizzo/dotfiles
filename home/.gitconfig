[user]
	name = Fabiano Frizzo
	email = ffrizzo@gmail.com
[color]
	diff = auto
	status = auto
	branch = auto
	interactive = auto
[merge]
	summary = true
[core]
	excludesfile = ~/.gitignore-global
	editor = code -n -w
[push]
	default = current
[diff]
	renames = copies
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
[mergetool "sourcetree"]
	trustExitCode = true
[pull]
	rebase = true
[include]
	path = 	~/.gitconfig-confidential
